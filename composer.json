{
    "name": "intelligents-dev/aia-connector",
    "description": "SDK to communicatie with the AIA-service",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "IntelligentsDev\\AiaConnector\\": "src/"
        }
    },
    "require": {
        "saloonphp/saloon": "^3.0",
        "saloonphp/laravel-http-sender": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "phpunit/phpunit": "^11.3"
    },
    "scripts": {
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "IntelligentsDev\\AiaConnector\\Providers\\AiaConnectorServiceProvider"
            ]
        }
    }
}
